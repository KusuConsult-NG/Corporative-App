rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /passports/{fileName} {
      allow read: if true;
      allow write: if request.auth != null && fileName.startsWith(request.auth.uid + "_");
    }
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
